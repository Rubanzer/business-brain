{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/health", "dest": "api/index.py" },
    { "src": "/analyze", "dest": "api/index.py" },
    { "src": "/context/file", "dest": "api/index.py" },
    { "src": "/context", "dest": "api/index.py" },
    { "src": "/csv", "dest": "api/index.py" },
    { "src": "/upload", "dest": "api/index.py" },
    { "src": "/chart", "dest": "api/index.py" },
    { "src": "/feed/(.*)", "dest": "api/index.py" },
    { "src": "/feed", "dest": "api/index.py" },
    { "src": "/reports/(.*)", "dest": "api/index.py" },
    { "src": "/reports", "dest": "api/index.py" },
    { "src": "/discovery/(.*)", "dest": "api/index.py" },
    { "src": "/suggestions", "dest": "api/index.py" },
    { "src": "/data/(.*)", "dest": "api/index.py" },
    { "src": "/metadata/(.*)", "dest": "api/index.py" },
    { "src": "/metadata", "dest": "api/index.py" },
    { "src": "/(.*)", "dest": "public/$1" }
  ]
}
